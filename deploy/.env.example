# Copy this file to deploy/.env and fill in your private values.
# deploy/.env is gitignored and must never be committed.

# Region targets (required)
US_WEST_HOST=YOUR_US_WEST_IP
US_WEST_USER=root
US_WEST_LABEL=us-west

ASIA_HOST=YOUR_ASIA_IP
ASIA_USER=root
ASIA_LABEL=asia

EUROPE_HOST=YOUR_EUROPE_IP
EUROPE_USER=root
EUROPE_LABEL=europe

# Deploy behavior (optional)
GEYSER_PARALLEL=1
GEYSER_PRESERVE_REMOTE_CONFIG=1
GEYSER_CONFIG_SRC=config/geyser.toml
